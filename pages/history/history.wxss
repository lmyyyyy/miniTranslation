/* * history.wxss * */

.history-list {
  display: flex;
  flex-direction: column;
  padding: 40rpx;
}
.header {
  display: flex;
}
.title {
  flex:1;
  font-size: 26rpx;
  color: #8995a1;
}
.logo-index {
  width: 500rpx;
  height: 230rpx; 
  margin-left: 140rpx;
}
.divline{
  background: #0033ff35;
  width: 100%;
  height: 8rpx;
}
.icon-close {
  margin-left: auto;
  color: #aaa;
  font-size: 26rpx;
}
.item {
  margin-top: 40rpx;
}

.item .query {
  color: #8995a1;
}

.item .result {
  margin-top: 16rpx;
}